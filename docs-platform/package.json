    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "typescript": "^5.3.2",
    "openapi-typescript": "^6.7.1",
    "swagger-parser": "^10.0.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
