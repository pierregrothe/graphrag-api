    "swagger-ui-react": "^5.10.3",
    "redoc": "^2.1.3",
    "@redocly/openapi-core": "^1.5.0",
    "graphql": "^16.8.1",
    "graphql-playground-react": "^1.7.28",
    "@graphql-tools/schema": "^10.0.0",
    "axios": "^1.6.2",
    "react-copy-to-clipboard": "^5.1.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-tabs": "^6.0.2",
    "react-select": "^5.8.0",
    "react-json-view": "^1.21.3",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4"
