    "@docusaurus/plugin-content-blog": "^3.0.1",
    "@docusaurus/plugin-content-pages": "^3.0.1",
    "@docusaurus/plugin-sitemap": "^3.0.1",
    "@docusaurus/theme-classic": "^3.0.1",
    "@docusaurus/theme-common": "^3.0.1",
