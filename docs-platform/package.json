{
  "name": "graphrag-api-docs",
  "version": "1.0.0",
  "description": "Enterprise-grade documentation platform for GraphRAG API",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "generate-api-docs": "node scripts/generate-api-docs.js",
    "update-openapi": "node scripts/update-openapi.js",
    "dev": "concurrently \"npm run start\" \"npm run watch-api\"",
    "watch-api": "nodemon --watch ../docs/api/openapi.yaml --exec \"npm run generate-api-docs\"",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@docusaurus/core": "^3.0.1",
    "@docusaurus/preset-classic": "^3.0.1",
    "@docusaurus/theme-search-algolia": "^3.0.1",
